<mvc:View controllerName="com.knpl.pragati.Training.controller.AddMaster" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:uxap="sap.uxap" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt" xmlns:ui="sap.ui.table" height="100%" xmlns:smartTable="sap.ui.comp.smarttable">

    <Page  title="Add Training Type" class="sapUiContentPadding" showNavButton="true" showHeader="true" navButtonPress="onNavBtnPress" >
        <content>
            <VBox class="sapUiSmallMargin">
                <f:SimpleForm id="SimpleFormChange354" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
                    <f:content>
                        <Label text="Training Type" />
                        <Input id="name" value="{oModelView>/FormData/TrainingType}" required="true" />
                    </f:content>
                </f:SimpleForm>
            </VBox>
        </content>
        <footer>
            <Toolbar>
				<ToolbarSpacer/>
				<Button width="9%" text= "Add" type="Accept" press="onPressSave" />
			</Toolbar>
        </footer>
    </Page>
</mvc:View>
