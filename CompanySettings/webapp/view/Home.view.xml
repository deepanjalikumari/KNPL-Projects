<mvc:View controllerName="com.knpl.pragati.CompanySettings.controller.Home" class="sapUiSizeCompact" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
xmlns:l="sap.ui.layout"
xmlns:core="sap.ui.core"
xmlns:f="sap.ui.layout.form">
	<Shell id="shell">
		<App id="app">
			<pages>
            <Page id="CompanySettings">
            
            <footer>
            <OverflowToolbar>
            
				<ToolbarSpacer/>
                

                <Button id="edit" text="Edit" type="Emphasized" press="handleEditPress" />
					
					<Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
                    <Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
                
                 
			</OverflowToolbar>
            </footer>
		</Page>
				
			</pages>
		</App>
	</Shell>
</mvc:View>
