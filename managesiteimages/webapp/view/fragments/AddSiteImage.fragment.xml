<core:FragmentDefinition xmlns:unified="sap.ui.unified" xmlns:upload="sap.m.upload" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
        <f:SimpleForm class="addForm" id="FormObjectData" editable="true" layout="ResponsiveGridLayout" title="{oModelControl>/Pagetitle}" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
            <f:content>
                <!-- <Label text="{i18n>Images}" required="true"/>
                <VBox>
                    <items>
                        <unified:FileUploader  change="onBannerImageFileChange" id="idImageUploader" width="100%" tooltip="{i18n>fileUploaderTrainingtip}" typeMissmatch="handleTypeMissmatch" fileType="png,jpg,jpeg" maximumFileSize="2.1" fileSizeExceed="onFileSizeExceed" useMultipart="false" sendXHR="true" fileAllowed="onUpload"/>
                        <Text text="{i18n>FileUploadMessage}"/>
                    </items>
                </VBox> -->

            <Table id="Table2" items="{path: 'oModelControl>/Table/Table2',  templateShareable:false}" width="auto" noDataText="{i18n>tableNoDataText}" growingScrollToLoad="false" busyIndicatorDelay="{oModelView>/tableBusyDelay}">
            <headerToolbar>
                <Toolbar>
                    <Title text="{i18n>Images}"/>
                    <RadioButtonGroup columns="2" select=".onRbRRDialogVolume" selectedIndex="{oModelView>/InputType}">
                        <buttons>
                            <RadioButton text="{i18n>RequiredVolume}"/>
                            <RadioButton text="{i18n>Table1Label2}"/>
                        </buttons>
                    </RadioButtonGroup>
                    <ToolbarSpacer />
                    <Button icon="sap-icon://add" text="{i18n>Add}" tooltip="{i18n>Add}" type="Emphasized" press="onPressAddRewardsV1('add')"/>
                </Toolbar>
            </headerToolbar>
            <columns>
                <Column visible="{= ${oModelView>/InputType}===0 }">
                    <Label text="{i18n>RequiredVolume}" visible="true" required="true"/>
                </Column>
                <Column visible="{= ${oModelView>/InputType}===1 }">
                    <Label text="{i18n>Table1Label2}" required="true" />
                </Column>
                <Column>
                    <Text text="{i18n>tblColAction}"/>
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <VBox>
                            <unified:FileUploader change="onBannerImageFileChange" id="idImageUploader" width="100%" tooltip="{i18n>fileUploaderTrainingtip}" typeMissmatch="handleTypeMissmatch" fileType="png,jpg,jpeg" maximumFileSize="2.1" fileSizeExceed="onFileSizeExceed" useMultipart="false" sendXHR="true" fileAllowed="onUpload"/>
                            <Text text="{i18n>FileUploadMessage}"/>
                        </VBox>
                        <HBox>
                            <Button visible="{= ${oModelControl>editable} !== true }" text="{i18n>Save}" tooltip="{i18n>Save}" type="Transparent" press="onPressSaveReward"></Button>
                            <Button icon="sap-icon://delete" type="Transparent" tooltip="{i18n>Delete}" press="onRemoveImage"></Button>
                        </HBox>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>

            </f:content>
        </f:SimpleForm>
</core:FragmentDefinition>