<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Painters"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="true"
		key="Id"
		descriptionKey="Name"
    
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearch">
				<fb:filterGroupItems>
					<!-- <fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="PainterId"
						label="Painter ID"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="PainterId" />
						</fb:control>
					</fb:FilterGroupItem> -->
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Name"
						label="Painter Name"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Name" />
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Mobile"
						label="Mobile"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Mobile" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
