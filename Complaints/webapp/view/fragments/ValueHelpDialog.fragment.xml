<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
      
		id="selectDialog"
		title="Painters"
        growing="true"
        growingThreshold="15"
		items="{path:'/PainterSet',sorter:{path:'Name'}}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">
		<StandardListItem 
			title="{Name}"
			description="Mob: {Mobile}" />
	</SelectDialog>
</core:FragmentDefinition>