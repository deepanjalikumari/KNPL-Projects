<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="Products"
         growing="true"
        
		class="sapUiPopupWithPadding"
		items="{path:'/MasterProductSet',sorter:{path:'ProductName'}}"
	    search="_handlePValueHelpSearch"
		confirm="_handleProdValueHelpConfirm"
        cancel="_handleProdValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{ProductName}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
