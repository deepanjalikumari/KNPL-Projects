<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="Packs"
        growing="true"
        growingThreshold="10"
		class="sapUiPopupWithPadding"
		items="{path:'/MasterRepProductSkuSet',parameters:{select:'SkuCode,Description'} }"
	    search="_handlePackValueHelpSearch"
		confirm="_handlePackValueHelpConfirm"
        cancel="_handleProdValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{Description}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
