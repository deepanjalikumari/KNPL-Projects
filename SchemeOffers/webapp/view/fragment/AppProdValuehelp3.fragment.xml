<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="Products"
        growing="true"
        growingThreshold="8"
		class="sapUiPopupWithPadding"
		items="{path:'/MasterProductSet',sorter:{path:'ProductName'},parameters:{select:'Id,ProductName'} }"
	    search="_handlePValueHelpSearch3"
		confirm="_handleProdValueHelpConfirm3"
        cancel="_handleProdValueHelpClose3"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{ProductName}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
