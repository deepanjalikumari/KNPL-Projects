<mvc:View controllerName="com.knpl.pragati.OrganizationSetup.controller.EditUser" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
xmlns:l="sap.ui.layout">
<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack">

	<l:VerticalLayout class="sapUiContentPadding" width="100%">
		<Label text="Name" labelFor="name" design="Bold" />
		<Input id="name" required="true" value="{data>Name}" />

		<Label text="Email" labelFor="email" design="Bold" />
		<Input id="email" required="true" value="{data>Email}"/>

        <Label text="DialCode" labelFor="dialcode" design="Bold" />
		<Input id="dialcode" required="true" value="{data>DialCode}"/>

         <Label text="Mobile" labelFor="mobile" design="Bold" />
		<Input id="mobile" required="true" value="{data>Mobile}" />

         <Label text="CountryCode" labelFor="countrycode" design="Bold" />
		<Input id="countrycode" required="true" value="{data>CountryCode}"/>

        <Button text="Update"
						press="update"
						ariaDescribedBy="defaultButtonDescription genericButtonDescription">
					
				</Button>
               
                
                 
	</l:VerticalLayout>
    </Page>
</mvc:View>
