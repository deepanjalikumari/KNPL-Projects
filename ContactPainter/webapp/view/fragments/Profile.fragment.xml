<core:FragmentDefinition xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
    <VBox width="100%">
        <f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="2" singleContainerFullSize="false">
            <f:content>
                <core:Title text="Membership"/>
                <Label text="Membership Id"/>
                <Text text="Sample Data"/>
                <Label required="true" text="{i18n>DateOfJoining}"/>
                <VBox height="auto" width="auto">
                    <DatePicker visible="{= ${oModelView>/profileEdit} === true }" placeholder="DD/MM/YYYY" value="{path:'oModelView>/PainterDetails/DateOfJoining',type : 'sap.ui.model.type.Date', formatOptions : { pattern: 'dd/MM/y' } }" required="true"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label required="true" text="{i18n>PrimaryMobileNo}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" type="Number" value="{path:'oModelView>/PainterDetails/PrimaryMobileNo',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>SecondryMobileNo}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" type="Number" value="{path:'oModelView>/PainterDetails/PrimaryMobileNo',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>AgeGroup}"/>
                <VBox height="auto" width="auto">
                    <ComboBox width="100%" visible="{= ${oModelView>/profileEdit} === true }" selectedKey="{path:'oModelView>/PainterDetails/AgeGroup',type : 'sap.ui.model.type.String', constraints:{  search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>PreferredLanguage}"/>
                <VBox height="auto" width="auto">
                    <ComboBox width="100%" visible="{= ${oModelView>/profileEdit} === true }" selectedKey="{path:'oModelView>/PainterDetails/PreferredLanguage',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <core:Title text="{i18n>PersonalDetails}"/>
                <Label text="{i18n>FullName}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" value="{path : 'oModelView>/PersonalDetails/FullName', type : 'sap.ui.model.type.String', constraints : {minLength :1 , maxLength :40, search :'^[A-Za-z]{1}$|^(?:[A-Za-z][ ]?[.]?[ ]?){1,40}[A-Za-z]$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label required="true" text="{i18n>EmailId}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" value="{ path : 'oModelView>/PersonalDetails/Email', type : 'sap.ui.model.type.String', constraints : { search : '^[\\w-]+(?:\\.[\\w-]+)*@(?:[\\w-]+\\.)+[a-zA-Z]{2,7}$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>LinkDealer}"/>
                <VBox height="auto" width="auto">
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>SecondryLinkDealer}"/>
                <VBox height="auto" width="auto">
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <core:Title text="{i18n>LocationDetails}"/>
                <Label text="{i18n>Area}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" value="{path : 'oModelView>/PersonalDetails/FullName', type : 'sap.ui.model.type.String', constraints : {minLength :1 , maxLength :40, search :'^[A-Za-z]{1}$|^(?:[A-Za-z][ ]?[.]?[ ]?){1,40}[A-Za-z]$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>City}"/>
                <VBox height="auto" width="auto">
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>State}"/>
                <VBox height="auto" width="auto">
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <core:Title text="{i18n>SecurityQuestions}"/>
                 <Label text="{i18n>Questions}"/>
                <VBox height="auto" width="auto">
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>Answer}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" value="{path : 'oModelView>/PersonalDetails/FullName', type : 'sap.ui.model.type.String', constraints : {minLength :1 , maxLength :40, search :'^[A-Za-z]{1}$|^(?:[A-Za-z][ ]?[.]?[ ]?){1,40}[A-Za-z]$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <core:Title text="{i18n>SegmentationDetails}"/>
                <Label text="{i18n>TimeSize}"/>
                <VBox>
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>Experiences}"/>
                <VBox>
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>SitePerMonth}"/>
                <VBox>
                    <ComboBox visible="{= ${oModelView>/profileEdit} === true }" width="100%" required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                        <items>
                            <core:Item text="Sample1" key="1"/>
                            <core:Item text="Sample2" key="2"/>
                        </items>
                    </ComboBox>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
                <Label text="{i18n>Potential}"/>
                <VBox height="auto" width="auto">
                    <Input visible="{= ${oModelView>/profileEdit} === true }" required="true" value="{path : 'oModelView>/PersonalDetails/FullName', type : 'sap.ui.model.type.String', constraints : {minLength :1 , maxLength :40, search :'^[A-Za-z]{1}$|^(?:[A-Za-z][ ]?[.]?[ ]?){1,40}[A-Za-z]$' } }"/>
                    <Text text="" visible="{= ${oModelView>/profileEdit} !== true }"/>
                </VBox>
            </f:content>
        </f:SimpleForm>
        <Table id="idFamilyDetils" width="auto" items="{path: 'oModelView>/FamilyDetails'}" noDataText="{i18n>tableNoDataText}" growingScrollToLoad="false" busyIndicatorDelay="{oModelView>/tableBusyDelay}">
            <headerToolbar>
                <Toolbar>
                    <Title text="{i18n>FamilyDetails}"/>
                    <ToolbarSpacer/>
                    <Button visible="{= ${oModelView>/profileEdit} === true }" text="{i18n>Add}" type="Emphasized" press="onPressAddFamliy" tooltip="{i18n>AddPainter}"></Button>
                </Toolbar>
            </headerToolbar>

            <columns>
                <Column>
                    <Text text="{i18n>SelectRelation}"/>
                </Column>
                <Column>
                    <Text text="{i18n>Name}"/>
                </Column>
                <Column>
                    <Text text="{i18n>ContactNumber}"/>
                </Column>
                <Column width="5%"></Column>
                <Column width="5%"></Column>

            </columns>

            <items>
                <ColumnListItem press="onListItemPress">
                    <cells>
                        <Input value="{oModelView>RelType}"/>
                        <Input value="{oModelView>Name}"/>
                        <Input value="{oModelView>ContactNumber}"/>
                        <Button icon="sap-icon://edit" type="Transparent" press="onPressEditRel"></Button>
                        <Button icon="sap-icon://delete" type="Transparent" press="onPressRemoveRel"></Button>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table width="auto" items="{path: 'oModelView>/AssetDetails'}" noDataText="{i18n>tableNoDataText}" growingScrollToLoad="false" busyIndicatorDelay="{oModelView>/tableBusyDelay}">
            <headerToolbar>
                <Toolbar>
                    <Title text="{i18n>AssetDetails}"/>
                    <ToolbarSpacer/>
                    <Button visible="{= ${oModelView>/profileEdit} === true }" text="{i18n>Add}" type="Emphasized" press=".onPressAdAsset" tooltip="{i18n>AddPainter}"></Button>
                </Toolbar>
            </headerToolbar>

            <columns>
                <Column>
                    <Text text="{i18n>TypeOfAsset}"/>
                </Column>
                <Column>
                    <Text text="{i18n>AssetName}"/>
                </Column>
                <Column width="5%"></Column>
                <Column width="5%"></Column>
            </columns>

            <items>
                <ColumnListItem press="onListItemPress">
                    <cells>
                        <Input value="{TypeOfAsst}"/>
                        <Input value="{Name}"/>
                        <Button icon="sap-icon://edit" type="Transparent" press="" tooltip="{i18n>AddPainter}"></Button>
                        <Button icon="sap-icon://delete" type="Transparent" press="onPressRemoveAsset" tooltip="{i18n>AddPainter}"></Button>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </VBox>
</core:FragmentDefinition>
