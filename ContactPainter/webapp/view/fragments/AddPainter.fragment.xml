<core:FragmentDefinition xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
    <VBox width="100%">
        <f:SimpleForm id="SimpleFormChange480_12120Dual" editable="true" layout="ResponsiveGridLayout" title="{i18n>AddPainterDetails}" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="2" singleContainerFullSize="false">
            <f:content>
                <core:Title text="{i18n>PainterDetails}"/>
                <Label text="{i18n>DateOfJoining}"/>
                <DatePicker placeholder="DD/MM/YYYY" value="{path:'oModelView>/PainterDetails/DateOfJoining',type : 'sap.ui.model.type.Date', formatOptions : { pattern: 'dd/MM/y' } }" required="true"/>
                <Label text="{i18n>PrimaryMobileNo}"/>
                <Input required="true" type="Number" value="{path:'oModelView>/PainterDetails/PrimaryMobileNo',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }"/>
                <Label text="{i18n>SecondryMobileNo}"/>
                <Input type="Number" value="{path:'oModelView>/PainterDetails/SecondryMobileNo',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }"/>
                <Label text="{i18n>AgeGroup}"/>
                <ComboBox selectedKey="{path:'oModelView>/PainterDetails/AgeGroup',type : 'sap.ui.model.type.String', constraints:{  search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>PreferredLanguage}"/>
                <ComboBox selectedKey="{path:'oModelView>/PainterDetails/PreferredLanguage',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <core:Title text="{i18n>PersonalDetails}"/>
                <Label text="{i18n>FullName}"/>
                <Input required="true" value="{path : 'oModelView>/PersonalDetails/FullName', type : 'sap.ui.model.type.String', constraints : {minLength :1 , maxLength :40, search :'^[A-Za-z]{1}$|^(?:[A-Za-z][ ]?[.]?[ ]?){1,40}[A-Za-z]$' } }"/>
                <Label text="{i18n>EmailId}"/>
                <Input value="{ path : 'oModelView>/PersonalDetails/Email', type : 'sap.ui.model.type.String', constraints : { search : '^[\\w-]+(?:\\.[\\w-]+)*@(?:[\\w-]+\\.)+[a-zA-Z]{2,7}$' } }"/>
                <Label text="{i18n>LinkDealer}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/PersonalDetails/PrimaryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>SecondryLinkDealer}"/>
                <ComboBox selectedKey="{path:'oModelView>/PersonalDetails/SecondryDealer',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <core:Title text="{i18n>LocationDetails}"/>
                <Label text="{i18n>Area}"/>
                <Input required="true" maxLength="60" value="{path : 'oModelView>/PainterLocation/Area', type : 'sap.ui.model.type.String', constraints : {search :'^[a-zA-Z0-9]+[!@#$&amp;().+,-/;/:/\/\'/&quot; ]*([A-Za-z]+[!@#$&amp;().+,-/;/:/\/\'/&quot; ]*)*'}}"/>
                <Label text="{i18n>City}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/PainterLocation/City',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>State}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/PainterLocation/State',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <core:Title text="{i18n>SecurityQuestions}"/>
                <Label text="{i18n>Questions}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/SecurityQuestions/Question',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>Answer}"/>
                <Input required="true" maxLength="50" value="{path : 'oModelView>/SecurityQuestions/Answer', type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z0-9]{1}[wW]*'} }"/>
                <core:Title text="{i18n>SegmentationDetails}"/>
                <Label text="{i18n>TimeSize}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/SegmentationDetails/TimeSize',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>Experiences}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/SegmentationDetails/Experiences',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>SitePerMonth}"/>
                <ComboBox selectedKey="{path:'oModelView>/SegmentationDetails/SitesPerMonth',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>Potential}"/>
                <Input required="false" maxLength="50" value="{path : 'oModelView>/SegmentationDetails/Potential', type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z0-9]{1}[wW]*'} }"/>

            </f:content>
        </f:SimpleForm>
        <Table id="idFamilyDetils" width="auto" items="{path: 'oModelView>/FamilyDetails'}" noDataText="{i18n>tableNoDataText}" growingScrollToLoad="false" busyIndicatorDelay="{oModelView>/tableBusyDelay}">
            <headerToolbar>
                <Toolbar>
                    <Title text="{i18n>FamilyDetails}"/>
                    <ToolbarSpacer/>
                    <Button text="{i18n>Add}" type="Emphasized" press="onPressAddFamliy" tooltip="{i18n>AddPainter}"></Button>
                </Toolbar>
            </headerToolbar>

            <columns>
                <Column>
                    <Text text="{i18n>SelectRelation}"/>
                </Column>
                <Column>
                    <Text text="{i18n>Name}"/>
                </Column>
                <Column>
                    <Text text="{i18n>ContactNumber}"/>
                </Column>
                <Column width="10%">
                    <Text text="{i18n>Actions}"/>
                </Column>


            </columns>

            <items>
                <ColumnListItem press="onListItemPress">
                    <cells>
                        <VBox>
                            <ComboBox editable="{= ${oModelView>editable} === true }" selectedKey="{path:'oModelView>RelType',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                                <items>
                                    <core:Item text="Sample1" key="1"/>
                                    <core:Item text="Sample2" key="2"/>
                                </items>
                            </ComboBox>
                        </VBox>
                        <VBox>
                            <Input required="true" value="{path:'oModelView>Name',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" visible="{= ${oModelView>editable} === true }"/>
                            <Text text="{oModelView>Name}" visible="{= ${oModelView>editable} !== true }"/>
                        </VBox>
                        <VBox>
                            <Input required="true" type="Number" value="{path:'oModelView>ContactNumber',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }" visible="{= ${oModelView>editable} === true }"/>
                            <Text text="{oModelView>ContactNumber}" visible="{= ${oModelView>editable} !== true }"/>
                        </VBox>
                        <HBox>
                            <Button visible="{= ${oModelView>editable} !== true }" icon="sap-icon://edit" type="Transparent" press="onPressEditRel"></Button>
                            <Button visible="{= ${oModelView>editable} === true }" text="{i18n>Save}" type="Transparent" press="onPressFDLSave"></Button>
                            <Button icon="sap-icon://delete" type="Transparent" press="onPressRemoveRel"></Button>
                        </HBox>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table width="auto" items="{path: 'oModelView>/AssetDetails'}" noDataText="{i18n>tableNoDataText}" growingScrollToLoad="false" busyIndicatorDelay="{oModelView>/tableBusyDelay}">
            <headerToolbar>
                <Toolbar>
                    <Title text="{i18n>AssetDetails}"/>
                    <ToolbarSpacer/>
                    <Button text="{i18n>Add}" type="Emphasized" press=".onPressAdAsset" tooltip="{i18n>AddPainter}"></Button>
                </Toolbar>
            </headerToolbar>

            <columns>
                <Column>
                    <Text text="{i18n>TypeOfAsset}"/>
                </Column>
                <Column>
                    <Text text="{i18n>AssetName}"/>
                </Column>
                <Column width="10%">
                    <Text text="{i18n>Actions}"/>
                </Column>

            </columns>

            <items>
                <ColumnListItem press="onListItemPress">
                    <cells>
                        <VBox>
                            <ComboBox editable="{= ${oModelView>editable} === true }" selectedKey="{path:'oModelView>TypeOfAsst',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                                <items>
                                    <core:Item text="Sample1" key="1"/>
                                    <core:Item text="Sample2" key="2"/>
                                </items>
                            </ComboBox>
                        </VBox>
                        <VBox>
                            <Input required="true" value="{path:'oModelView>Name',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" visible="{= ${oModelView>editable} === true }"/>
                            <Text text="{oModelView>Name}" visible="{= ${oModelView>editable} !== true }"/>
                        </VBox>
                        <HBox>
                            <Button visible="{= ${oModelView>editable} !== true }" icon="sap-icon://edit" type="Transparent" press="onAssetEdit"></Button>
                            <Button visible="{= ${oModelView>editable} === true }" text="{i18n>Save}" type="Transparent" press="onAsetSave"></Button>
                            <Button icon="sap-icon://delete" type="Transparent" press="onPressRemoveAsset"></Button>
                        </HBox>

                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="2" singleContainerFullSize="false">
            <f:content>
                <core:Title text="{i18n>BankingDetails}"/>
                <Label text="{i18n>AccountHolderName}"/>
                <Input required="true" value="{path:'oModelView>/BankingDetails/AccountHolderName',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" />
                <Label text="{i18n>AccountType}"/>
                <ComboBox required="true" selectedKey="{path:'oModelView>/BankingDetails/AccountType',type : 'sap.ui.model.type.String', constraints:{ search: '^[0-9]{1}$' } }">
                    <items>
                        <core:Item text="Sample1" key="1"/>
                        <core:Item text="Sample2" key="2"/>
                    </items>
                </ComboBox>
                <Label text="{i18n>BankName}"/>
                <Input required="true" value="{path:'oModelView>/BankingDetails/BankName',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" />
                <Label text="{i18n>AccountNumber}"/>
                <Input required="true" type="Number" value="{path:'oModelView>/BankingDetails/AccountNumber',type : 'sap.ui.model.type.String', constraints:{ minLength:10,maxLength:20, search: '^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$' } }" />
                <Label text="{i18n>IFSCCode}"/>
                <Input required="true" value="{path:'oModelView>/BankingDetails/IFSCCode',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" />
                <core:Title text="{i18n>KYCDetails}"/>
                <Label text="{i18n>KYCStatus}"/>
                <Input required="false" value="{path:'oModelView>/BankingDetails/KYCStatus',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" />
                <Label text="{i18n>AadhaarCardNumber}"/>
                <Input required="false" value="{path:'oModelView>/BankingDetails/AadhaarCardNo',type : 'sap.ui.model.type.String', constraints : {minLength:1, search:'^[a-zA-Z]{1}[wW]*'} }" />
            </f:content>
        </f:SimpleForm>

    </VBox>
</core:FragmentDefinition>
