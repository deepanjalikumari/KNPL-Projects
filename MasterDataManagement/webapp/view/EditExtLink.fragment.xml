<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox
            
    >
		<f:SimpleForm 
			editable="true"
			layout="ColumnLayout"
            id="idForm"
			columnsM="2"
			columnsL="2"
			columnsXL="2"
			>
			<f:content>
				<Label text="{i18n>MasterDataDescription}" />
				<Input value="{tableData>desc}"/>
                <Label text="{i18n>MasterDataLink}" />
				<Input value="{tableData>link}"/>        
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>